    def mudarConfigSistema(self):
        index = self.cb_metodo.currentIndex()
        metodo = ""
        hora_ad = self.ins_hora_ad.text()
        hora_c = self.ins_hora_c.text()
        hora_m = self.ins_hora_m.text()
        hora_o = self.ins_hora_o.text()
        tolerancia = self.ins_tolerancia.text()
        pernoite = self.ins_pernoite.text()
        hora_ini_pernoite = self.ins_h_ini_pernoite.text()
        hora_ter_pernoite = self.ins_h_ter_pernoite.text()
        valor_mensal_c = self.ins_valor_mensal_c.text()
        valor_mensal_m = self.ins_valor_mensal_m.text()
        valor_mensal_o = self.ins_valor_mensal_o.text()
        
        for m,valor in self.metodo.items():
            if valor == index:
                metodo = m
                break
        
        self.config.set('sistema','metodo',metodo)
        self.config.set('sistema','valor_hora_ad',hora_ad)
        self.config.set('sistema','valor_hora_c',hora_c)
        self.config.set('sistema','valor_hora_m',hora_m)
        self.config.set('sistema','valor_hora_o',hora_o)
        self.config.set('sistema','tolerancia',tolerancia)
        self.config.set('sistema','valor_pernoite',pernoite)
        self.config.set('sistema','hora_ini_pernoite',hora_ini_pernoite)
        self.config.set('sistema','hora_term_pernoite',hora_ter_pernoite)
        self.config.set('sistema','valor_mensal_c',valor_mensal_c)
        self.config.set('sistema','valor_mensal_m',valor_mensal_m)
        self.config.set('sistema','valor_mensal_o',valor_mensal_o)
        
        with open('config/config.ini', 'w') as configfile:
            self.config.write(configfile)
            
        self.inf()        
     
