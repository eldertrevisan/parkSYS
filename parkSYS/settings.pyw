    def lerConfig(self):
        indexEmp = 0
        indexSis = 0
        for uf,valor in self.uf.items():
            if uf == self.config.get('empresa','uf'):
                indexEmp = valor
                break
            
        for metodo,valor in self.metodo.items():
            if metodo == self.config.get('sistema','metodo'):
                indexSis = valor
                break
        
        #LEITURA DAS CONFIGURAÇÕES DA EMPRESA        
        self.ins_razao.setText(self.config.get('empresa','razao_social'))
        self.ins_nome.setText(self.config.get('empresa','nome_fantasia'))
        self.ins_cnpj.setText(self.config.get('empresa','cnpj'))
        self.ins_ie.setText(self.config.get('empresa','ie'))
