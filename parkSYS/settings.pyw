    def mudarConfigEmpresa(self):            
        razao = self.ins_razao.text()
        nome = self.ins_nome.text()
        cnpj = self.ins_cnpj.text()
        ie = self.ins_ie.text()
        endereco = self.ins_endereco.text()
        num = self.ins_num.text()
        bairro = self.ins_bairro.text()
        cep = self.ins_cep.text()
        cidade = self.ins_cidade.text()
        index = self.cb_uf.currentIndex()
        uf = ""        
        
        for u,valor in self.uf.items():
            if valor == index:
                uf = u
                break
        
        self.config.set('empresa','razao_social',razao)
        self.config.set('empresa','nome_fantasia',nome)
        self.config.set('empresa','cnpj',cnpj)
        self.config.set('empresa','ie',ie)
        self.config.set('empresa','endereco',endereco)
        self.config.set('empresa','num',num)
        self.config.set('empresa','bairro',bairro)
        self.config.set('empresa','cep',cep)
        self.config.set('empresa','cidade',cidade)
        self.config.set('empresa','uf',uf)
        
        with open('config/config.ini', 'w') as configfile:
            self.config.write(configfile)
            
        self.inf()
        
