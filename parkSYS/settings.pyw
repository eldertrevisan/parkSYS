        self.ins_num.setText(self.config.get('empresa','num'))
        self.ins_bairro.setText(self.config.get('empresa','bairro'))
        self.ins_cep.setText(self.config.get('empresa','cep'))
        self.ins_cidade.setText(self.config.get('empresa','cidade'))
        self.cb_uf.setCurrentIndex(indexEmp)
        
