    #MÉTODO PARA DESBLOQUEAR OS CAMPOS DE NOME/RAZÃO SOCIAL E NOME FANTASIA CONFORME SELEÇÃO DO COMBOBOX
    def cb_tipo_p(self, arg):        
        self.ins_nome_rz_social.setReadOnly(True)
        self.ins_nome_fantasia.setReadOnly(True)
        if arg == "Particular":
            self.ins_nome_rz_social.setReadOnly(False)
        elif arg == "Empresa":
            self.ins_nome_rz_social.setReadOnly(False)
            self.ins_nome_fantasia.setReadOnly(False)
        else:
            self.limpa_campos_cliente()

    #MÉTODO PARA INSERIR VEÍCULOS NA TABELA
    def insere_veiculos(self):
        cb_tipo_veiculo = self.cb_tipo_veiculo.currentText()
        ins_marca = self.ins_marca.text()
        ins_modelo = self.ins_modelo.text()
        ins_placa = self.ins_placa.text()
        ins_cor = self.ins_cor.text()
        ins_chassi = self.ins_chassi.text()            
        lista = (cb_tipo_veiculo, ins_marca, ins_modelo, ins_placa, ins_cor, ins_chassi)
        self.table.insertRow(self.table.rowCount())
        qtdRows = self.table.rowCount()
        j = 0
        for i in lista:
            item = QtGui.QTableWidgetItem()
            item.setText(str(i))
            item.setTextAlignment(QtCore.Qt.AlignCenter)
            self.table.setItem(qtdRows-1,j,item)
            j += 1
            
        self.cb_tipo_veiculo.setFocus()            
        self.limpa_campos_veiculo()
       
    #MÉTODO PARA GRAVAR OS DADOS DO CLIENTE NO BANCO DE DADOS
    def grava_dados(self):
        self.msg_sistema.setText(self.msg[2])
        
        tipo_pessoa = self.cb_tipo_pessoa.currentText()
        ins_nome_rz_social = self.ins_nome_rz_social.text()
        ins_nome_fantasia = self.ins_nome_fantasia.text()
        ins_tel1 = self.ins_tel1.text()
        ins_tel2 = self.ins_tel2.text()
        ins_cel = self.ins_cel.text()
        ins_email1 = self.ins_email1.text()
        ins_email2 = self.ins_email2.text()
        ins_endereco = self.ins_endereco.text()
        ins_bairro = self.ins_bairro.text()
        ins_cidade = self.ins_cidade.text()
        cb_uf = self.cb_uf.currentText()
        ins_rg_ie = self.ins_rg_ie.text()
        ins_cpf_cnpj = self.ins_cpf_cnpj.text()  #CPF OU CNPJ SERÁ O CÓDIGO DO CLIENTE
        ct_informacoes = self.ct_informacoes.toPlainText()
        
        #PEGA OS DADOS DO(S) VEÍCULOS DA TABELA E ARMAZENA EM UMA LISTA
        col = self.table.columnCount()
        lin = self.table.rowCount()
        dados_veic = []
        for l in range(lin):
            dados = []
            for c in range(col):
                dados.append(self.table.item(l,c).text())
            dados_veic.append(dados)
        
        #VERIFICA SE ESTES CAMPOS ESTÃO SEM PREENCHER
        #CASO NÃO ESTEJAM, NÃO DEIXA GRAVAR OS DADOS    
        if self.ins_nome_rz_social.text() == '' and \
            self.ins_tel1.text() == '() -' and \
            self.ins_email1.text() == '' and \
            self.ins_endereco.text() == '' and \
            self.ins_bairro.text() == '' and \
            self.ins_cidade.text() == '' and \
            self.cb_uf.currentText() == '' and \
            self.ins_rg_ie.text() == '' and \
            self.ins_cpf_cnpj.text() == '' and \
            self.ins_marca.text() == '' and \
            self.ins_modelo.text() == '' and \
            self.ins_placa.text() == '-':
                
            self.msg_sistema.setText(self.msg[0])
            
        else:
            #SE FOR CADASTRO NOVO EXECUTA A AÇÃO ABAIXO
            if self.verifica_bt == True:
                dados_cliente = (None, tipo_pessoa, ins_nome_rz_social,\
                        ins_nome_fantasia, ins_tel1, ins_tel2, ins_cel, ins_email1, ins_email2,\
                        ins_endereco, ins_bairro, ins_cidade, cb_uf, ins_rg_ie, ins_cpf_cnpj, ct_informacoes)
                banco.insere_mensalista(dados_cliente, dados_veic)
            #SE FOR ATUALIZAÇÃO DE CADASTRO EXECUTA A AÇÃO ABAIXO
            elif self.verifica_bt == False:
                cb_cliente = self.cb_carrega_clientes.currentText()
                cod_cliente = None
                for cliente,cod in self.cliente_cod.items():
                    if cliente == cb_cliente:
                        cod_cliente = cod
                dados_cliente = (cod_cliente, tipo_pessoa, ins_nome_rz_social,\
                        ins_nome_fantasia, ins_tel1, ins_tel2, ins_cel, ins_email1, ins_email2,\
                        ins_endereco, ins_bairro, ins_cidade, cb_uf, ins_rg_ie, ins_cpf_cnpj, ct_informacoes)
                banco.atualiza_dados_cliente(dados_cliente, dados_veic)
            else:
                pass
        #APÓS O PROCESSO DE CADASTRO OU ATUALIZAÇÃO, EXIBE MENSAGEM DE CONCLUSÃO
        #E CHAMA OS MÉTODOS PARA LIMPEZA DOS CAMPOS DO FORMULÁRIO
        self.msg_sistema.setText(self.msg[1])        
        self.limpa_campos_cliente()
        self.limpa_campos_veiculo()
            
    #MÉTODO PARA LIMPAR OS CAMPOS DO CLIENTE NO FORMULÁRIO
    def limpa_campos_cliente(self):
        self.cb_carrega_clientes.setCurrentIndex(0)
        self.cb_tipo_pessoa.setCurrentIndex(0)
        self.ins_nome_rz_social.clear()
        self.ins_nome_fantasia.clear()
        self.ins_tel1.clear()
        self.ins_tel2.clear()
        self.ins_cel.clear()
        self.ins_email1.clear()
        self.ins_email2.clear()
        self.ins_endereco.clear()
        self.ins_bairro.clear()
        self.ins_cidade.clear()
        self.cb_uf.setCurrentIndex(0)
        self.ins_rg_ie.clear()
        self.ins_cpf_cnpj.clear()
        self.ct_informacoes.clear()
        #REMOVE TODAS AS LINHAS DA TABELA DE VEÍCULOS
        lt = self.table.rowCount()
        while lt >= 0:
            self.table.removeRow(lt)
            lt -= 1
    
