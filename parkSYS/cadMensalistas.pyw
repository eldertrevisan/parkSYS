    #MÉTODO PARA CARREGAR OS CLIENTES DO BANCO E EXIBIR NO COMBOBOX
    def carrega_clientes_banco(self):
        self.cliente_cod = {}
        clientes = banco.carrega_clientes()
        k = 0
        self.cb_carrega_clientes.addItem("")
        for i in clientes:
            self.cliente_cod[i[0]] = i[1]
            k += 1
            self.cb_carrega_clientes.addItem("")
            self.cb_carrega_clientes.setItemText(k, QtGui.QApplication.translate("CadMensalistas", i[0], None, QtGui.QApplication.UnicodeUTF8))
    
    #BUSCA OS DADOS DO CLIENTE E DE SEU(S) VEÍCULO(S) NO BANCO E EXIBE NO FORMULÁRIO
    def carrega_clientes(self, arg):
        for cliente,cod in self.cliente_cod.items():
            if cliente == arg:
                dados_cliente, dados_veiculos = banco.carrega_clientes_completo(cod)
                for i in dados_cliente:                    
                    cb_tipo_pessoa = i[1]
                    for tipo_pessoa,index in self.tipos_pessoas.items():
                        if cb_tipo_pessoa == tipo_pessoa:
                            self.cb_tipo_pessoa.setCurrentIndex(index)
                    self.ins_nome_rz_social.setText(i[2])
                    self.ins_nome_fantasia.setText(i[3])
                    self.ins_tel1.setText(i[4])
                    self.ins_tel2.setText(i[5])
                    self.ins_cel.setText(i[6])
                    self.ins_email1.setText(i[7])
                    self.ins_email2.setText(i[8])
                    self.ins_endereco.setText(i[9])
                    self.ins_bairro.setText(i[10])
                    self.ins_cidade.setText(i[11])
                    cb_uf = (i[12])
                    for uf,index in self.uf.items():
                        if cb_uf == uf:
                            self.cb_uf.setCurrentIndex(index)
                    self.ins_rg_ie.setText(str(i[13]))
                    self.ins_cpf_cnpj.setText(str(i[14]))
                    self.ct_informacoes.setPlainText(i[15])
                
                lt = self.table.rowCount()
                while lt >= 0:
                    self.table.removeRow(lt)
                    lt -= 1
                    self.msg_sistema.setText(self.msg[2])
                j = 0
                qtdRows = self.table.rowCount()
                for k in dados_veiculos:
                    self.table.insertRow(self.table.rowCount())
                    for items in k[2:]:
                        item = QtGui.QTableWidgetItem()
                        item.setText(str(items))
                        item.setTextAlignment(QtCore.Qt.AlignCenter)
                        self.table.setItem(qtdRows,j,item)
                        j += 1
        if self.cb_carrega_clientes.currentIndex() == 0:
            self.verifica_bt = True
            self.bt_salvar.setEnabled(True)
            self.bt_alterar.setEnabled(False)
            self.limpa_campos_cliente()
            self.limpa_campos_veiculo()
        else:
            self.bt_salvar.setEnabled(False)
            self.bt_alterar.setEnabled(True)
            self.verifica_bt = False
    
